		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.7.3",
		"uuid": "^11.0.5",
		"vercel": "^41.1.0",
		"vite": "^6.1.0",
		"vitest": "^3.0.5"
	},
	"dependencies": {
		"bits-ui": "^1.0.0-next.80",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.5.2",
		"formsnap": "^2.0.0",
		"google-auth-library": "^9.15.1",
		"google-spreadsheet": "^4.1.4",
		"lucide-svelte": "^0.475.0",
		"mode-watcher": "^0.5.1",
		"svelte-meta-tags": "^4.1.0",
		"svelte-sonner": "^0.3.28",
